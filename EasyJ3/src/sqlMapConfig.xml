<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig
PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
<settings
    cacheModelsEnabled="true"
    enhancementEnabled="true"
    lazyLoadingEnabled="true"
    maxRequests="32"
    maxSessions="10"
    maxTransactions="5"
    useStatementNamespaces="true"
/>
<!-- 
<typeHandler javaType="String" callback="elicitation.utils.StringTypeHandlerCallback"/>-->
<transactionManager type="JDBC" >
    <dataSource type="SIMPLE">
      <property name="JDBC.Driver" value="com.mysql.jdbc.Driver"/>
      <property name="JDBC.ConnectionURL" value="jdbc:mysql://192.168.4.133:3306/refactor?characterEncoding=utf8"/>
      <property name="JDBC.Username" value="admin"/>
      <property name="JDBC.Password" value="123"/>
      
    </dataSource>
</transactionManager>
<sqlMap resource="ibatis-user.xml" />
<sqlMap resource="ibatis-project.xml"/>
<sqlMap resource="ibatis-domain.xml"/>
<sqlMap resource="ibatis-request.xml"/>
<sqlMap resource="ibatis-question.xml"/>
<sqlMap resource="ibatis-solution.xml"/>
<sqlMap resource="ibatis-version.xml"/>
<sqlMap resource="ibatis-review.xml"/>
</sqlMapConfig>
